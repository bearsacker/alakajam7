global int u = 56786
global int v = 837689

function Random:randSeed(seed) {
	if seed == null {
		seed = 0
	}

	u = seed + 56786
	v = seed + 837689
}

function Random:randSeed() {
	return u - 56786
}

function Random:rand() {
	u = 36969 * (u & 65535) + (u >> 16)
    v = 18000 * (v & 65535) + (v >> 16)

    return (u << 16) + v
}

function Random:rand(max) {
	return Random:rand() % max
}

function Random:rand(min, max) {
	return min + (Random:rand() % (max - min))
}